#include <stdio.h>

// Função para calcular a soma dos elementos do array
int soma_array(int arr[], int tamanho) {
    int soma = 0;
    for (int i = 0; i < tamanho; i++) {
        soma += arr[i];
    }
    return soma;
}

int main() {
    int arr[] = {5, 7, 9, 6};
    int tamanho = sizeof(arr) / sizeof(arr[0]);
    int resultado = soma_array(arr, tamanho);

    printf("Soma dos elementos do array: %d\n", resultado);

    return 0;
}
